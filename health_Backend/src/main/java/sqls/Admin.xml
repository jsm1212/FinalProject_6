<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="health.back.a.dao.WorkBbsDao">
	<!-- 회원 목록 -->
	<select id="getMem" resultType="health.back.a.dto.LoginMemberDto">
		SELECT * FROM LOGINMEMBERS
		WHERE DEL=0
		ORDER BY ID
	</select>
	
	<!-- 회원 삭제 -->
	<update id="deleteMem" parameterType="java.lang.String">
		UPDATE LOGINMEMBERS SET DEL=1
		WHERE ID=#{id}
	</update>
</mapper>